        "df[\"ref_summary_len\"] = df[\"ref_summary\"].astype(str).apply(lambda x: len(x.split()))\n",
        "df[\"pred_summary_len\"] = df[\"pred_summary\"].astype(str).apply(lambda x: len(x.split()))\n",
