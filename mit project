          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Topic metrics: {'macro': {'precision': 0.0, 'recall': 0.0, 'f1': 0.0}, 'micro': {'precision': 0.0, 'recall': 0.0, 'f1': 0.0}}\n"
          ]
        },
