        "top_pred = Counter(all_pred_topics).most_common(30)\n",
        "top_df = pd.DataFrame(top_pred, columns=[\"topic\",\"count\"])\n",
        "try_display_df(\"Top_predicted_topics_after_beam\", top_df)"
